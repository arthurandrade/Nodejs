{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "globals": {
        "require": true
    },
    "rules": {
        "comma-dangle": [
            "error",
            {
                "arrays": "always-multiline",
                "objects": "always-multiline",
                "imports": "always-multiline",
                "exports": "always-multiline",
                "functions": "never"
            }
        ]
    }
}
